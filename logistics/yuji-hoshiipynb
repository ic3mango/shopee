    "df['1st_deliver_attempt'] = df['1st_deliver_attempt'].apply(lambda x: datetime.fromtimestamp(x).date())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def augment(x):\n",
    "    try:\n",
    "        y = datetime.fromtimestamp(x).date()\n",
    "        return y\n",
    "    except:\n",
    "        return pd.NaT"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "df['2nd_deliver_attempt'] = df['2nd_deliver_attempt'].apply(lambda x: augment(x))"
   ]
