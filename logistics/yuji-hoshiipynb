  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "def augment2(first, second, holidays):\n",
    "    try:\n",
    "        return np.busday_count(first, second, holidays=holidays, weekmask='1111110')\n",
    "    except:\n",
    "        return np.datetime64('NaT')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "df['2nd_deliver_time'] = df.apply(lambda x: augment2(x['1st_deliver_attempt'], x['2nd_deliver_attempt'], holidays), axis=1)"
   ]
