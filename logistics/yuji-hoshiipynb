  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "def delivery_window(df2):\n",
    "    df2.buyeraddress = df2.buyeraddress.str.lower()\n",
    "    df2.selleraddress = df2.selleraddress.str.lower()\n",
    "\n",
    "\n",
    "    for location in ['metro manila', 'luzon', 'visayas', 'mindanao']:\n",
    "        df2['Buyer_{}'.format(location)] = 0\n",
    "        df2.loc[df2.buyeraddress.str.endswith('{}'.format(location)), 'Buyer_{}'.format(location)] = 1\n",
    "    \n",
    "        df2['Seller_{}'.format(location)] = 0\n",
    "        df2.loc[df2.selleraddress.str.endswith('{}'.format(location)), 'Seller_{}'.format(location)] = 1\n",
    "    # All time allocation is 7 days\n",
    "    df2['time_allocation'] = 7\n",
    "\n",
    "    # except for these\n",
    "    df2.loc[(df2['Buyer_metro manila'] == 1) & (df2['Seller_metro manila'] == 1),'time_allocation'] = 3\n",
    "    df2.loc[(df2['Buyer_metro manila'] == 1) & (df2['Seller_luzon'] == 1),'time_allocation'] = 5\n",
    "    df2.loc[(df2['Buyer_luzon'] == 1) & (df2['Seller_metro manila'] == 1),'time_allocation'] = 5\n",
    "    df2.loc[(df2['Buyer_luzon'] == 1) & (df2['Seller_luzon'] == 1),'time_allocation'] = 5\n",
    "    \n",
    "    return df2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "df2 = delivery_window(df)"
   ]
